config ISCSI_TARGET
	tristate "Linex-iSCSI.org iSCSI Target Mode Stack"
	depends on INET
	select CRYPTO
	select CRYPTO_CRC32C
	select CRYPTO_CRC32C_INTEL if X86
	help
	Say M here to enable the ConfigFS enabled Linex-iSCSI.org iSCSI
	Target Mode Stack.

source	"drivers/target/iscsi/cxgbit/Kconfig"
